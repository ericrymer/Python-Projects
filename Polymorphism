
#Parent Class
class Animal:
#Function
  def intro(self):
    print("There are many types of animals that walk the Earth.")
     
  def swim(self):
    print("Some can walk, and some can swim. Not many can do both!")
#Child Class   
class penguin(Animal):
  def swim(self):
    print("Penguin's can swim underwater for long distances and walk on land!")
#Child Class        
class squirrel(Animal):
  def swim(self):
    print("Squrriels are not very good swimmers, they wouldn't last long in a body of water.")
     
obj_animal = Animal()
obj_pen = penguin()
obj_sq = squirrel()

#Displays Parent Class 
obj_animal.intro()
obj_animal.swim()
#Displays Child Class 
obj_pen.intro()
obj_pen.swim()
#Displays Child Class 
obj_sq.intro()
obj_sq.swim()
 
